{
  "processes": [
    {
      "env": {
        "NODE_ENV": "production"
      }
      "id": "app",
      "cmd": "deno run -A main.ts",
      "autostart": true,
      "restart": "error",
      "cluster": {
        "instances": 4
      }
    }
  ],
  "plugins": [
    {
      "url": "jsr:@pup/plugin-web-interface",
      "options": {
        "port": 5000
      }
    }
  ]
}
